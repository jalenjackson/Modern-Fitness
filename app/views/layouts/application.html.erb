<!DOCTYPE html>
<html>
  <head>
    <title>ModernFitnessProject</title>
    <%= csrf_meta_tags %>
    <% if content_for?(:head) %>
        <%= yield(:head) %>
    <% end %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag 'home' %>
    <%= stylesheet_link_tag 'articles' %>
    <%= stylesheet_link_tag 'profiles' %>
    <%= stylesheet_link_tag 'alertify.min' %>
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/jquery.slick/1.8.0/slick.css"/>
    <link href="https://fonts.googleapis.com/css?family=Josefin+Sans:600" rel="stylesheet">
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
